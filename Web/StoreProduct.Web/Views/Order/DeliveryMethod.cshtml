@model DeliveryModel
@{ 
    ViewData["Title"] = "Выбор способа доставки";
}

<h1>Способ доставки</h1>

<form>
    <input class="hidden" name="id" value="@Model.OrderId"/>
    <div>
        <label form="code">Способ доставки:</label>
        <select class="form-control" name="uniquecode">
            @foreach(var e in Model.Method)
            {
                <option value="@Model.Method.Keys">@Model.Method.Values</option>
            }
        </select>
    </div>

    <button class="btn btn-succes" type="submit">Далее</button>
</form> 